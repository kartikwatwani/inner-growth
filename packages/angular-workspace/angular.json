{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "inner-growth-angular": {
      "projectType": "library",
      "root": "projects/inner-growth-angular",
      "sourceRoot": "projects/inner-growth-angular/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/inner-growth-angular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/inner-growth-angular/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/inner-growth-angular/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/inner-growth-angular/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "9528a1b9-8b7a-40c8-93b8-1c6d068bb7de"
  }
}
